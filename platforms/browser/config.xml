<?xml version='1.0' encoding='utf-8'?>
<widget id="com.creative.helpvox2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Helpvox</name>
    <description>
        Empresa mais completa em acessibilidade para surdos e deficientes auditivos do mundo, utiliza os serviços de acessibilidade do Helpvox!
    </description>
    <author email="contato@creativenegocios.com" href="http://www.creativenegocios.com">
        Creative Negócios
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
